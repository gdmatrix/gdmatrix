<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config xmlns="http://java.sun.com/JSF/Configuration">

  <managed-bean>
    <managed-bean-name>navigatorBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.webapp.NavigatorBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>     

  <managed-bean>
    <managed-bean-name>caseObjectBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.webapp.modules.cases.CaseObjectBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>     
  
  <managed-bean>
    <managed-bean-name>caseMainTabBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.webapp.modules.cases.CaseMainTabBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>          
  
  <managed-bean>
    <managed-bean-name>casePersonsTabBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.webapp.modules.cases.CasePersonsTabBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>caseDocumentsTabBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.webapp.modules.cases.CaseDocumentsTabBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
    
  <managed-bean>
    <managed-bean-name>caseBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CaseBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>     
  
  <managed-bean>
    <managed-bean-name>caseSearchBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CaseSearchBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>     
    
  <managed-bean>
    <managed-bean-name>caseMainBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CaseMainBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>          
  
  <managed-bean>
    <managed-bean-name>casePersonsBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CasePersonsBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>caseAddressesBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CaseAddressesBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>  
  
  <managed-bean>
    <managed-bean-name>caseCasesBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CaseCasesBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>    
  
  <managed-bean>
    <managed-bean-name>caseInterventionsBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CaseInterventionsBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>   
  
  <managed-bean>
    <managed-bean-name>caseDocumentsBacking</managed-bean-name>
    <managed-bean-class>org.matrix.pf.cases.CaseDocumentsBacking</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>    

  <navigation-rule>
    <from-view-id>*</from-view-id>
    
    <navigation-case>
      <from-outcome>pf_case_search</from-outcome>
      <to-view-id>/pf/common/cases/case_search.xhtml</to-view-id>
    </navigation-case> 
    
    <navigation-case>
      <from-outcome>pf_case_main</from-outcome>
      <to-view-id>/pf/common/cases/case_main.xhtml</to-view-id>
    </navigation-case>    
    
    <navigation-case>
      <from-outcome>pf_case_persons</from-outcome>
      <to-view-id>/pf/common/cases/case_persons.xhtml</to-view-id>
    </navigation-case>      
    
    <navigation-case>
      <from-outcome>pf_case_addresses</from-outcome>
      <to-view-id>/pf/common/cases/case_addresses.xhtml</to-view-id>
    </navigation-case>               

    <navigation-case>
      <from-outcome>pf_case_cases</from-outcome>
      <to-view-id>/pf/common/cases/case_cases.xhtml</to-view-id>
    </navigation-case>  
    
    <navigation-case>
      <from-outcome>pf_case_interventions</from-outcome>
      <to-view-id>/pf/common/cases/case_interventions.xhtml</to-view-id>
    </navigation-case>      
    
    <navigation-case>
      <from-outcome>pf_case_documents</from-outcome>
      <to-view-id>/pf/common/cases/case_documents.xhtml</to-view-id>
    </navigation-case>                                                                                                                                                                                      
                                   
  </navigation-rule>

</faces-config>
