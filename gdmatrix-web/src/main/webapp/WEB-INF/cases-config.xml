    <managed-bean-name>personCasesBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.PersonCasesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>caseProblemsBean</managed-bean-name>
