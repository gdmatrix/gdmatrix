  </managed-bean>

  <managed-bean>
    <managed-bean-name>caseMainBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.CaseMainBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>caseDetailBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.detail.CaseDetailBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>mainPanelBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.detail.MainPanelBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>browserPanelBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.detail.BrowserDetailPanel</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>documentsPanelBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.detail.DocumentsPanelBean</managed-bean-class>
