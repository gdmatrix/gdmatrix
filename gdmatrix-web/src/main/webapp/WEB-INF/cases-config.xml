    <managed-bean-class>org.santfeliu.cases.web.detail.GalleryPanelBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>workflowsPanelBean</managed-bean-name>
