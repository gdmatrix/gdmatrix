  </managed-bean>

  <managed-bean>
    <managed-bean-name>caseTreeBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.CaseTreeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>interventionSearchBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.InterventionSearchBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>  

  <navigation-rule>
    <from-view-id>*</from-view-id>

    <navigation-case>
      <from-outcome>case_search</from-outcome>
      <to-view-id>/common/cases/case_search.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_main</from-outcome>
      <to-view-id>/common/cases/case_main.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_detail</from-outcome>
      <to-view-id>/common/cases/case_detail.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>panels_documents</from-outcome>
      <to-view-id>/common/cases/panels/documents.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_persons</from-outcome>
      <to-view-id>/common/cases/case_persons.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_addresses</from-outcome>
      <to-view-id>/common/cases/case_addresses.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_events</from-outcome>
      <to-view-id>/common/cases/case_events.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_documents</from-outcome>
      <to-view-id>/common/cases/case_documents.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_interventions</from-outcome>
      <to-view-id>/common/cases/case_interventions.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>person_cases</from-outcome>
      <to-view-id>/common/cases/person_cases.xhtml</to-view-id>
    </navigation-case>    

    <navigation-case>
      <from-outcome>case_problems</from-outcome>
      <to-view-id>/common/cases/case_problems.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_demands</from-outcome>
      <to-view-id>/common/cases/case_demands.xhtml</to-view-id>
    </navigation-case>
<!--    
    <navigation-case>
      <from-outcome>case_globalView</from-outcome>
      <to-view-id>/common/cases/case_globalView.jsp</to-view-id>
    </navigation-case>
-->
    <navigation-case>
      <from-outcome>case_acl</from-outcome>
      <to-view-id>/common/cases/case_acl.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_cases</from-outcome>
      <to-view-id>/common/cases/case_cases.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_tree</from-outcome>
      <to-view-id>/common/cases/case_tree.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>address_cases</from-outcome>
      <to-view-id>/common/cases/address_cases.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>intervention_search</from-outcome>
      <to-view-id>/common/cases/intervention_search.xhtml</to-view-id>
    </navigation-case>    

  </navigation-rule>
</faces-config>
