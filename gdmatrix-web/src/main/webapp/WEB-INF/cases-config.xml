  </managed-bean>

  <managed-bean>
    <managed-bean-name>caseACLBean</managed-bean-name>
    <managed-bean-class>org.santfeliu.cases.web.CaseACLBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>caseCasesBean</managed-bean-name>
