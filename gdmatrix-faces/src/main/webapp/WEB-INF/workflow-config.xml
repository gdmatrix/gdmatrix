 <navigation-rule>
   <from-view-id>*</from-view-id>
   
   <navigation-case>
     <from-outcome>start_instance</from-outcome>
     <to-view-id>/common/workflow/start_instance.jsp</to-view-id>
   </navigation-case>

   <navigation-case>
     <from-outcome>procedure_catalogue</from-outcome>
     <to-view-id>/common/workflow/procedure_catalogue.jsp</to-view-id>
   </navigation-case>

   <navigation-case>
     <from-outcome>procedure_info</from-outcome>
     <to-view-id>/common/workflow/procedure_info.jsp</to-view-id>
   </navigation-case>

   <navigation-case>
     <from-outcome>instance_list</from-outcome>
     <to-view-id>/common/workflow/instance_list.jsp</to-view-id>
   </navigation-case>

   <navigation-case>
     <from-outcome>not_logged_in</from-outcome>
     <to-view-id>/common/workflow/not_logged_in.jsp</to-view-id>
   </navigation-case>

   <navigation-case>
     <from-outcome>instance_terminated</from-outcome>
     <to-view-id>/common/workflow/instance_terminated.jsp</to-view-id>
   </navigation-case>

   <navigation-case>
     <from-outcome>nothing_to_do</from-outcome>
     <to-view-id>/common/workflow/nothing_to_do.jsp</to-view-id>
   </navigation-case>

   <navigation-case>
