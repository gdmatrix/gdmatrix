    <navigation-case>
      <from-outcome>person_search</from-outcome>
      <to-view-id>/common/kernel/person_search.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>person_main</from-outcome>
      <to-view-id>/common/kernel/person_main.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>person_contacts</from-outcome>
      <to-view-id>/common/kernel/person_contacts.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>person_representants</from-outcome>
