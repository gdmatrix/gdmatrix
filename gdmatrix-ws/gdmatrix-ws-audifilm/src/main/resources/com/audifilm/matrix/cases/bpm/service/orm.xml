  <named-query name="findCaseStates">
    <query>
      SELECT cs
      FROM CaseState cs
      WHERE
        (cs.caseId = :caseId OR :caseId IS NULL)
        AND (cs.stateId = :stateId OR :stateId IS NULL)
        AND (:dateTime IS NULL
           OR (:dateTime &gt;= CONCAT(cs.startDate, cs.startTime)
              AND (:dateTime &lt;= CONCAT(cs.endDate, cs.endTime) OR cs.endDate IS NULL)))
      ORDER BY cs.caseId asc, cs.startDate, cs.startTime
    </query>
  </named-query>

  <named-query name="findLastCaseState" >
    <query>
      SELECT cs
      FROM CaseState cs
      WHERE
        (cs.caseId = :caseId)
        AND (cs.endDate IS NULL OR cs.stateId = '9999')
        AND cs.stateId NOT LIKE '@%'
      ORDER BY cs.startDate desc, cs.startTime desc
    </query>
  </named-query>



  <named-query name="findCasePersons" >
    <query>
      SELECT c
      FROM Case c
      WHERE
        (c.caseId = :caseId OR :caseId IS NULL)
        AND (c.persId = :personId OR c.reprId = :personId OR :personId IS NULL)
    </query>
  </named-query>

  <named-query name="countCasePersons" >
    <query>
      SELECT count(c)
      FROM Case c
      WHERE
        (c.caseId = :caseId OR :caseId IS NULL)
        AND (c.persId = :personId OR c.reprId = :personId OR :personId IS NULL)
    </query>
  </named-query>

  <named-query name="findInteressats" >
