  <named-query name="findSituacions" >
    <query>
      SELECT si
      FROM Situacio si
      WHERE
        (si.caseId = :caseId OR :caseId IS NULL)
         AND (si.dataAcceptacio IS NULL OR :tot IS NULL OR :tot = 'S')
    </query>
  </named-query>

  <named-query name="incrementGenericCounter">
    <query>
      UPDATE Counter c
      SET
        c.counter = c.counter + 1,
        c.stdumod = :userId,
        c.stddmod = :day,
        c.stdhmod = :hour
      WHERE
        trim(c.claupref) = trim(:claupref)
        AND trim(c.claucod) = trim(:claucod)
        AND trim(c.clauorigen) = trim(:clauorigen)
    </query>
  </named-query>

  <named-query name="readGenericCounter">
    <query>
      SELECT c.counter
      FROM Counter c
      WHERE
        trim(c.claupref) = trim(:claupref)
        AND trim(c.claucod) = trim(:claucod)
        AND trim(c.clauorigen) = trim(:clauorigen)
    </query>
  </named-query>


  <named-query name="findCaseDocument" >
    <query>
      SELECT
        cd
      FROM
        CaseDocument cd
      WHERE
        (:caseId is Null OR trim(cd.caseId) = :caseId)
        AND (:aplcod is Null OR trim(cd.aplcod) = :aplcod)
        AND (:docorigen is Null OR trim(cd.docorigen) = :docorigen)
        AND (:doccod is Null OR trim(cd.doccod) = :doccod)
        AND (:modelcod is Null OR trim(cd.modelcod) = :modelcod)
        AND (:docnompc is Null OR trim(cd.docnompc) = :docnompc)
        AND (:identificador is Null OR trim(cd.identificador) = :identificador)
      ORDER BY cd.caseId, cd.docnompc
     </query>
  </named-query>

  <named-query name="findCaseDocumentViews" >
    <query>
      SELECT
        c, cd
      FROM
        Case c, CaseDocument cd
      WHERE
        trim(c.caseId) = trim(cd.caseId)
        AND NOT (cd.docId is NULL)
        AND (:caseId is NULL OR c.caseId = :caseId)
        AND (:docId is NULL OR cd.docId = :docId)
      ORDER BY c.caseId, cd.docnompc
     </query>
  </named-query>
  
 <named-query name="findCaseVariables" >
    <query>
      SELECT
        cv, v
      FROM
        CaseVariable cv, Variable v
      WHERE
        cv.caseId = :caseId
        AND trim(cv.caseTypeId) = v.caseTypeId
        AND trim(cv.variableId) = v.variableId
        AND cv.x = 0 AND cv.y = 0 AND cv.tramcod = 0
      ORDER BY cv.caseId, cv.caseTypeId, cv.variableId
    </query>
  </named-query>

  <named-query name="countCaseVariables" >
    <query>
      SELECT
        count(cv)
      FROM
        CaseVariable cv
      WHERE
        cv.caseId = :caseId
    </query>
  </named-query>

  <named-query name="findCaseState" >
    <query>
      SELECT cs
      FROM CaseState cs
      WHERE
        cs.caseId = :caseId
        AND cs.endDate is null
     </query>
  </named-query>


  <named-query name="findCaseStateResponsible" >
    <query>
      SELECT csr
      FROM CaseStateResponsible csr , CaseState cs
      WHERE csr.caseId = :caseId
       AND csr.caseId = cs.caseId
       AND csr.caseStateReferece = cs.caseStateReferece
       AND cs.endDate is null
    </query>
  </named-query>


  <named-query name="findCaseClassId" >
    <query>
      SELECT ap.classId
      FROM Case c, AplDocument ap
      WHERE
        trim(ap.aplId) = 'SDE'
        AND trim(ap.docorigen) = 'EXPED'
        AND trim(ap.docId) = trim(c.caseTypeId)
        AND (c.caseId = :caseId)
        AND NOT ap.classId is null
    </query>
  </named-query>
  
  <named-native-query name="findNativeCaseDocumentViews">
    <query>
      SELECT c.SDENUM,c.TEXPCOD,c.SDENUMTEXP,c.SDETEXT,c.sdedreg,c.sdehreg,c.stdugr,c.stdumod,c.stddgr,c.stddmod,c.stdhgr,c.stdhmod,
        cd.APLCOD,cd.DOCORIGEN,cd.DOCCOD,cd.MODELCOD,cd.DOCNOMPC,cd.IDENTIFICADOR,cd.ID_NREG,cd.SDENUM as caseId
      FROM
        SDE_EXPEDIENT c, APL_DOCSAPL cd 
      WHERE (c.sdenum = cd.sdenum AND cd.docorigen = 'EXPED'
         AND (cd.eliminat = 0 OR cd.eliminat is null))  
         AND (cd.docnompc not like 'tmp%')              
         AND NOT (cd.id_nreg is NULL)                   
         AND c.sdenum = ?caseId
      UNION   
      SELECT c.SDENUM,c.TEXPCOD,c.SDENUMTEXP,c.SDETEXT,c.sdedreg,c.sdehreg,c.stdugr,c.stdumod,c.stddgr,c.stddmod,c.stdhgr,c.stdhmod,
        cd.APLCOD,cd.DOCORIGEN,cd.DOCCOD,cd.MODELCOD,cd.DOCNOMPC,cd.IDENTIFICADOR,cd.ID_NREG,cd.SDENUM as caseId
      FROM
        SDE_EXPEDIENT c, RES_ENTRELEXP re, APL_DOCSAPL cd 
      WHERE (c.sdenum = re.sdenum AND re.resnume = cd.IDENTIFICADOR AND cd.docorigen = 'REGENT' 
         AND (cd.eliminat = 0 OR cd.eliminat is null))
         AND (cd.docnompc not like 'tmp%')
         AND NOT (cd.id_nreg is NULL)
         AND c.sdenum = ?caseId
      ORDER BY SDENUM, ID_NREG, DOCNOMPC
    </query>
  </named-native-query>  
  
