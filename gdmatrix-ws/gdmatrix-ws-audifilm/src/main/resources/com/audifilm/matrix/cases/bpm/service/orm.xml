  <named-query name="findCaseCases" >
    <query>
     SELECT cc, c, rc
     FROM CaseCase cc JOIN cc.mainCase c JOIN cc.relCase rc
     WHERE
       (cc.caseId = :caseId OR :caseId IS NULL)
       AND (cc.relCaseId = :relCaseId OR :relCaseId IS NULL)
     ORDER BY cc.caseId
    </query>
  </named-query>

  <named-query name="countCaseCases" >
    <query>
     SELECT count(cc)
     FROM CaseCase cc
     WHERE
       (cc.caseId = :caseId OR :caseId IS NULL)
       AND (cc.relCaseId = :relCaseId OR :relCaseId IS NULL)
    </query>
  </named-query>

  <named-query name="countCaseAddresses" >
    <query>
      SELECT count(ca)
      FROM CaseAddress ca
      WHERE
        (ca.caseId = :caseId OR :caseId IS NULL)
        AND (ca.addressId = :addressId
            OR :addressId IS NULL)
    </query>
  </named-query>

  <named-query name="findCaseAddresses" >
    <query>
      SELECT ca
      FROM CaseAddress ca
      WHERE
        (ca.caseId = :caseId OR :caseId IS NULL)
        AND 
        (ca.addressId = :addressId OR :addressId IS NULL)
      ORDER BY ca.caseId, ca.addressId
    </query>
  </named-query>

  <named-query name="countCaseAddressCases" >
    <query>
